func mat_mul(array array int A, array array int B) -> array array int
    /// assert(A[0].size == B.size);
    array array int C := [[0; 3]; 3];
    for i from 0 to 3 do
        for j from 0 to 3 do
            C[i][j] := 0;
            for k from 0 to 3 do
                C[i][j] := C[i][j] + A[i][k]*B[k][j];
            end
        end
    end
    return C;
end